/*Tarea*/
/* 1. Mostrar la lista de estudiantes con su respectivo nombre de escuela */
SELECT 
    estudiante.codigo_estudiante,
    estudiante.ap_estudiante,
    estudiante.am_estudiante,
    estudiante.nombre,
    escuela.nombre_escuela
FROM estudiante
INNER JOIN escuela ON estudiante.codigo_escuela = escuela.codigo_escuela


/* 2. Obtener todos los nombres de estudiantes incluyendo el nombre de la carrera profesional,
      de aquellos que tienen más de 3 hermanos */
SELECT 
    estudiante.codigo_estudiante,
    estudiante.ap_estudiante + ' ' + estudiante.am_estudiante + ' ' + estudiante.nombre AS nombre_completo,
    escuela.nombre_escuela AS carrera_profesional,
    estudiante.nroHermanos
FROM estudiante
INNER JOIN escuela ON estudiante.codigo_escuela = escuela.codigo_escuela
WHERE estudiante.nroHermanos > 3


/* 3. Mostrar la escuela y los estudiantes que pertenecen a la escuela de Ing. Informática y Sistemas */
SELECT 
    escuela.nombre_escuela,
    estudiante.codigo_estudiante,
    estudiante.ap_estudiante + ' ' + estudiante.am_estudiante + ' ' + estudiante.nombre AS nombre_completo
FROM escuela
INNER JOIN estudiante ON escuela.codigo_escuela = estudiante.codigo_escuela
WHERE escuela.nombre_escuela = 'Ingeniería Informática y Sistemas'


/* 4. Mostrar todos los docentes que dictan clases en el semestre 1 (adaptado a 2024) */
SELECT DISTINCT
    docente.codigo_docente,
    docente.ap_docente + ' ' + docente.am_docente + ' ' + docente.nombre_docente AS nombre_completo_docente,
    docente.titulo_docente,
    cargaHorario.semestre
FROM docente
INNER JOIN cargaHorario ON docente.codigo_docente = cargaHorario.codigo_docente
WHERE cargaHorario.semestre = 1


/* 5. Recuperar el nombre y el apellido de las escuelas que tienen más de 5 estudiantes */
SELECT 
    escuela.codigo_escuela,
    escuela.nombre_escuela,
    COUNT(estudiante.codigo_estudiante) AS total_estudiantes
FROM escuela
INNER JOIN estudiante ON escuela.codigo_escuela = estudiante.codigo_escuela
GROUP BY escuela.codigo_escuela, escuela.nombre_escuela
HAVING COUNT(estudiante.codigo_estudiante) > 5


/* 6. Seleccionar nombre_escuela, ap_estudiante, am_estudiante, nombres, estudiante, sexo
      de los estudiantes masculinos */
SELECT 
    escuela.nombre_escuela,
    estudiante.ap_estudiante,
    estudiante.am_estudiante,
    estudiante.nombre AS nombres_estudiante,
    estudiante.codigo_estudiante AS estudiante,
    estudiante.sexo
FROM estudiante
INNER JOIN escuela ON estudiante.codigo_escuela = escuela.codigo_escuela
WHERE estudiante.sexo = 'M'


/* 7. Mostrar todos los Planes de estudio con sus respectivas asignaturas */
SELECT 
    planEstudio.codigo_planEstudio,
    planEstudio.observaciones_planEstudio,
    escuela.nombre_escuela,
    asignatura.codigo_asignatura,
    asignatura.nombre_asignatura,
    asignatura.cat_asignatura,
    asignatura.credito_asignatura
FROM planEstudio
INNER JOIN escuela ON planEstudio.codigo_escuela = escuela.codigo_escuela
INNER JOIN asignatura ON planEstudio.codigo_planEstudio = asignatura.codigo_planEstudio


/* 8. Calcular cuantos estudiantes hay por escuela, debe mostrar el nombre de la escuela */
SELECT 
    escuela.nombre_escuela,
    COUNT(estudiante.codigo_estudiante) AS total_estudiantes
FROM escuela
LEFT JOIN estudiante ON escuela.codigo_escuela = estudiante.codigo_escuela
GROUP BY escuela.codigo_escuela, escuela.nombre_escuela
ORDER BY total_estudiantes DESC


/* 9. Contar la cantidad de estudiantes matriculados en el semestre 1 (adaptado) */
SELECT 
    COUNT(DISTINCT matricula.codigo_estudiante) AS total_estudiantes_matriculados,
    matricula.semestre
FROM matricula
WHERE matricula.semestre = 1
GROUP BY matricula.semestre


/* 10. Calcular la cantidad de matriculados por Escuela en el semestre 1 (adaptado) */
SELECT 
    escuela.nombre_escuela,
    COUNT(DISTINCT matricula.codigo_estudiante) AS total_matriculados,
    matricula.semestre
FROM matricula
INNER JOIN estudiante ON matricula.codigo_estudiante = estudiante.codigo_estudiante
INNER JOIN escuela ON estudiante.codigo_escuela = escuela.codigo_escuela
WHERE matricula.semestre = 1
GROUP BY escuela.codigo_escuela, escuela.nombre_escuela, matricula.semestre
ORDER BY total_matriculados DESC


/* 11. Mostrar aquellas escuelas que tienen más de 3 matriculados */
SELECT 
    escuela.nombre_escuela,
    COUNT(DISTINCT matricula.codigo_estudiante) AS total_matriculados
FROM escuela
INNER JOIN estudiante ON escuela.codigo_escuela = estudiante.codigo_escuela
INNER JOIN matricula ON estudiante.codigo_estudiante = matricula.codigo_estudiante
GROUP BY escuela.codigo_escuela, escuela.nombre_escuela
HAVING COUNT(DISTINCT matricula.codigo_estudiante) > 3
ORDER BY total_matriculados DESC


/* 12. Calcular el promedio de hermanos de todos los estudiantes matriculados 
       de la Escuela de Ing. Informática y Sistemas */
SELECT 
    escuela.nombre_escuela,
    AVG(CAST(estudiante.nroHermanos AS FLOAT)) AS promedio_hermanos
FROM estudiante
INNER JOIN escuela ON estudiante.codigo_escuela = escuela.codigo_escuela
INNER JOIN matricula ON estudiante.codigo_estudiante = matricula.codigo_estudiante
WHERE escuela.nombre_escuela = 'Ingeniería Informática y Sistemas'
GROUP BY escuela.nombre_escuela


/* 13. Calcular la media de notas de ingreso por escuela 
       (Nota: La base de datos no tiene campo 'ciudad', se calcula por escuela) */
SELECT 
    escuela.nombre_escuela,
    AVG(CAST(estudiante.notaIngreso AS FLOAT)) AS media_nota_ingreso
FROM estudiante
INNER JOIN escuela ON estudiante.codigo_escuela = escuela.codigo_escuela
GROUP BY escuela.codigo_escuela, escuela.nombre_escuela
ORDER BY media_nota_ingreso DESC


/* 14. Mostrar el nombre de los estudiantes que obtuvieron la mejor nota de ingreso por escuela */
SELECT 
    escuela.nombre_escuela,
    estudiante.ap_estudiante + ' ' + estudiante.am_estudiante + ' ' + estudiante.nombre AS nombre_completo,
    estudiante.notaIngreso AS nota_maxima
FROM estudiante
INNER JOIN escuela ON estudiante.codigo_escuela = escuela.codigo_escuela
WHERE estudiante.notaIngreso = (
    SELECT MAX(estudiante2.notaIngreso)
    FROM estudiante estudiante2
    WHERE estudiante2.codigo_escuela = estudiante.codigo_escuela
)
ORDER BY escuela.nombre_escuela [42000-240]
